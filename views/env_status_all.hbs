{
{{>common}}
"attachments": [
    {{#each envs}}
        {
        "mrkdwn_in": ["text"],
        {{#if val}}
            "color": "warning",
            "text": "*{{key}}* is taken by {{val}}."
        {{else}}
            "color": "good",
            "text": "*{{key}}* is available."
        {{/if}}
        },
    {{/each}}
    {
        "mrkdwn_in": ["text"],
        {{#if last}}
            {{#if last.val}}
                "color": "warning",
                "text": "*{{last.key}}* is taken by {{last.val}}."
            {{else}}
                "color": "good",
                "text": "*{{last.key}}* is available."
            {{/if}}
        {{else}}
            "color": "danger",
            "text": "No suitable envs found"
        {{/if}}
    }
    ]
}